page {
  background-color: #F3F3F3;
}
.paddingView .sticky {
  position: -webkit-sticky;
  position: sticky;
  top: 87rpx;
  padding: 40rpx 28rpx;
  background-color: #F3F3F3;
}
.paddingView .sticky .searchBox {
  overflow: hidden;
  -webkit-border-radius: 12rpx;
          border-radius: 12rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  background-color: #EBEBEB;
}
.paddingView .sticky .searchBox .searchIcon {
  width: 36rpx;
  height: 36rpx;
  margin-left: 28rpx;
}
.paddingView .sticky .searchBox .input {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  width: 252rpx;
  height: 40rpx;
  font-size: 28rpx;
  margin: 0 18rpx;
}
.paddingView .sticky .searchBox .placeholder {
  color: #BEBEBE;
}
.paddingView .sticky .searchBox .searchButton {
  background: -webkit-gradient(linear, left top, right top, from(#28A032), to(#50C414));
  background: -webkit-linear-gradient(left, #28A032 0%, #50C414 100%);
  background: linear-gradient(90deg, #28A032 0%, #50C414 100%);
  color: #FFFFFF;
  margin-left: auto;
  width: 120rpx;
  height: 70rpx;
  font-size: 32rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.paddingView .list {
  margin-top: 40rpx;
}
.paddingView .list .item {
  background-color: #FFFFFF;
  -webkit-border-radius: 12rpx;
          border-radius: 12rpx;
  padding: 24rpx 40rpx 0;
}
.paddingView .list .item .it {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  font-weight: bold;
}
.paddingView .list .item .it .left {
  font-size: 28rpx;
  color: #666666;
}
.paddingView .list .item .it .right {
  font-size: 28rpx;
